{
  "name": "mz-ui",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
   "keywords": [
    "nutui",
    "vue",
    "vue3",
    "webpack",
    "mzui",
    "vue3 component",
    "vite"
  ],
   "author": "xiao",
  "license": "MIT",
   "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "npm run checked && vite --open --force",
    "build2": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "checked": "npm run generate:file && tsc",
    "generate:file": "node bin/generate-mzui.js",
    "generate:types": "node bin/generate-types.js",
    "generate:themes": "node bin/generate-themes.js",
    "build": "npm run checked && vite build --config vite.config.build.ts && vite build --config vite.config.build.disperse.ts && npm run generate:types && npm run generate:themes && vite build --config vite.config.build.css.ts"

  },
  "dependencies": {
    "highlight.js": "^11.5.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "fs-extra": "^10.0.1",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.0",
    "autoprefixer": "^10.4.4",
    "sass": "^1.49.11",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vite-plugin-md": "^0.12.4",
    "vue-tsc": "^0.29.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "vue/no-unused-components": "off",
      "no-debugger": "off"
    }
  }
}
